+                <p className="text-sm text-gray-600">Created: {new Date(plan.created).toLocaleDateString()}</p>
+              </div>
+
+              <div>
+                <h4 className="font-semibold text-gray-900 mb-3">‚úÖ Do now (under 5 min):</h4>
+                <ul className="space-y-2">
+                  {plan.doNow.map(step => (
+                    <li key={step.id} className="flex items-start gap-2 text-sm">
+                      <span className="text-green-600 mt-0.5">‚Ä¢</span>
+                      <span>{step.title}</span>
+                    </li>
+                  ))}
+                </ul>
+              </div>
+
+              <div>
+                <h4 className="font-semibold text-gray-900 mb-3">üî® Build this week:</h4>
+                <ul className="space-y-2">
+                  {plan.buildThisWeek.map(step => (
+                    <li key={step.id} className="flex items-start gap-2 text-sm">
+                      <span className="text-blue-600 mt-0.5">‚Ä¢</span>
+                      <span>{step.title}</span>
+                    </li>
+                  ))}
+                </ul>
+              </div>
+
+              <div className="bg-purple-50 p-4 rounded-lg border border-purple-200">
